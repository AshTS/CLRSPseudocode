CountingSort(A, B, k)
    C = ArrayCreate(k + 1)

    for i = 1 to k + 1
        C[i] = 0

    for j = 1 to A.length
        C[A[j] + 1] = C[A[j] + 1] + 1
    
    for i = 2 to k + 1
        C[i] = C[i] + C[i - 1]

    for j = A.length down to 1
        B[C[A[j] + 1]] = A[j]
        C[A[j] + 1] = C[A[j] + 1] - 1

Test()
    A = Array(1, 2, 3, 3, 1, 1, 2)
    B = Array(0, 0, 0, 0, 0, 0, 0)

    Print(A)

    CountingSort(A, B, 3)

    Print(B)